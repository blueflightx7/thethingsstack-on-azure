{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Dependency"
            }
        },
        "logLevel": {
            "default": "Information",
            "Host.Results": "Information",
            "Host.Aggregator": "Information",
            "Function": "Information",
            "Function.ProcessToSQL": "Information",
            "Function.TtsBridge": "Information"
        }
    },
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[4.*, 5.0.0)"
    }
}